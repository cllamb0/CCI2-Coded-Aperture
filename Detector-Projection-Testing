  {
   "cell_type": "code",
   "execution_count": 141,
   "id": "4326c9ba",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cart2sph(coord):\n",
    "    x, y, z = coord\n",
    "    if x > 0:\n",
    "        phi = np.degrees(np.arctan(y/x))\n",
    "    elif x < 0 and y >= 0:\n",
    "        phi = np.degrees(np.arctan(y/x) + np.pi) \n",
    "    elif x < 0 and y < 0:\n",
    "        phi = np.degrees(np.arctan(y/x) - np.pi)\n",
    "    elif x == 0 and y > 0:\n",
    "        phi = np.degrees(np.pi/2)\n",
    "    elif x == 0 and y < 0:\n",
    "        phi = np.degree(-np.pi/2)\n",
    "    else:\n",
    "        phi = 0\n",
    "    transform = np.array([np.sqrt(x**2 + y**2 + z**2),\n",
    "                          np.degrees(np.arccos(z/np.sqrt((x**2 + y**2 + z**2)))),\n",
    "                          phi])\n",
    "    return transform"
   ]
  },
