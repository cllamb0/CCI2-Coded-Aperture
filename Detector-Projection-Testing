    "CA_mask = np.loadtxt('Optimizations-Desktop-Tetris/Optimizations-Tetris-300/GD_ms_46-of_3-mag_4-seed_44-hl_80-cw_1.0-sw_2.0-ft_0.4/final_mask.txt')\n",
    "CA_mask = np.where(CA_mask == 0, 0, 1)\n",
    "\n",
    "det_start = ((CA_mask.shape[0] - det_size) / 2) * mask_pix_size + (det_pix_size / 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 74,
   "id": "f60c715e",
   "metadata": {},
   "outputs": [],
   "source": [
    "det_x, det_y = np.mgrid[det_start:det_start+(det_size*det_pix_size):det_pix_size, det_start:det_start+(det_size*det_pix_size):det_pix_size]\n",
    "detector_pixels = np.hstack((np.array([det_x.flatten(), det_y.flatten()]).T, (np.ones(det_x.flatten().size)*(-(b-mask_thickness))).reshape(-1,1)))\n",
    "\n",
    "vox_start = -(100-(CA_mask.shape[0]*mask_pix_size))/2 + (voxel_size / 2) # 100 from 10cm extension @ m = 4 and b = 7cm\n",
    "vox_x, vox_y, vox_z = np.mgrid[vox_start:100+vox_start:voxel_size, vox_start:100+vox_start:voxel_size, -(b+z_depth+(voxel_size/2))+voxel_size:-(b+(voxel_size/2))+voxel_size:voxel_size]\n",
    "voxels = np.array([vox_x.flatten(), vox_y.flatten(), vox_z.flatten()]).T"
